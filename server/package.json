{
  "name": "shinchan",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node --experimental-specifier-resolution=node ./src/app",
    "serve": "node ./dist/bin/www",
    "dev": "nodemon --exec babel-node ./src/bin/www",
