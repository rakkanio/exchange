    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "glob": "^8.0.3",
    "ipfs": "^0.65.0",
    "ipfs-http-client": "^56.0.2",
    "jsonwebtoken": "^9.0.0",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.5.0",
    "moralis": "^2.10.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.31.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "eslint": "^6.5.1",
    "nodemon": "^2.0.15"
